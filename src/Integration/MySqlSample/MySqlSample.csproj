<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>MySqlSample</AssemblyName>
    <PackageId>MySqlSample</PackageId>
    <RuntimeIdentifiers>win</RuntimeIdentifiers>
  </PropertyGroup>

  <ItemGroup>
    <None Include="App.config" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="6.9.9" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="NServiceBus" Version="6.2.0" />
    <ProjectReference Include="..\Shared\Shared.csproj" />
    <Reference Include="NServiceBus.Persistence.Sql">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>..\..\ForIntegration\NServiceBus.Persistence.Sql.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net452' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
